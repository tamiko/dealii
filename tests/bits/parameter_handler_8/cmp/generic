
# Listing of Parameters
# ---------------------
subsection Testing testing
  # docs 3
  set double_double = 3.1415926
  set int/int       = 1

  # docs 1
  set string list   = a


  subsection Testing%testing
    # docs 3
    set double+double = 3.1415926 # default: 6.1415926
    set int*int       = 1         # default: 2

    # docs 1
    set string&list   = a         # default: a,b,c
  end

end


